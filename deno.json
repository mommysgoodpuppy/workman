{
  "imports": {
    "@mommysgoodpuppy/logchannel": "jsr:@mommysgoodpuppy/logchannel@^0.1.0",
    "@sigma/pty-ffi": "jsr:@sigma/pty-ffi@^0.39.1",
    "@std/io": "jsr:@std/io@^0.225.2",
    "std/": "https://deno.land/std@0.224.0/"
  },
  "tasks": {
    "run": "deno run --allow-read src/main.ts",
    "test": "deno test -A --no-check tests",
    "test-nova": "deno bundle -o wm_minimal.js ./wm_minimal.ts && andromeda run ./wm_minimal.js"
  },
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": false
  },
  "lint": {
    "rules": {
      "include": [
        "verbatim-module-syntax"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  }
}