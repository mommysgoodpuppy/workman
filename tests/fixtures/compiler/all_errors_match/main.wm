type ParseError = Missing | Other;

let classify = match(result) {
  IOk(value) => { value },
  IErr(Missing) => { Missing },
  AllErrors => { Other }
};

export let okCase = () => {
  classify(IOk(Missing))
};

export let missingCase = () => {
  classify(IErr(Missing))
};

export let otherCase = () => {
  classify(IErr(Other))
};
