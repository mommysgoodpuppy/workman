  "version": "0.2.0",
  "configurations": [

    {
      "request": "launch",
      "name": "Launch Program",
      "type": "node",
      "program": "${workspaceFolder}/wm.ts",
      "cwd": "${workspaceFolder}",
      "env": {},
      "runtimeExecutable": "C:/Users/Gamingprofile/.version-fox/cache/deno/current/deno.exe",
      "runtimeArgs": [
        "run",
        "--inspect-wait",
        "--allow-all"
      ],
      "args": [
        "err",
        "std/list.wm"
      ],
      "attachSimplePort": 9229
    }
  ]
}