-- Math utility module
export let square = (x) => {
  mul(x, x)
};

export let cube = (x) => {
  mul(x, mul(x, x))
};

export let sumOfSquares = (a, b) => {
  add(square(a), square(b))
};

-- Private helper (not exported)
let double = (x) => {
  add(x, x)
};

export let quadruple = (x) => {
  double(double(x))
};
